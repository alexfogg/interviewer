- @interview.name
.progress.hide= @progress.id
%br
- @interview.questions.each do |question|
  %br
  .q
    =question.question
    %br
    - question.answers.each do |answer|
      %br
      %input{:name => "is_correct", :type => "checkbox", :class => 'check'}
      = answer.response
    %br
    = link_to('Submit', create_progresses_path(@interview), :remote => true)



